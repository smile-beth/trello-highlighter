  "bugs": {
    "url": "https://github.com/smile-beth/trello-highlighter/issues"
  },
  "homepage": "https://github.com/smile-beth/trello-highlighter#readme",
  "description": "",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.16.1",
    "parcel": "^2.16.1"
  },
  "dependencies": {
    "cpx": "^1.5.0"
  }
}
