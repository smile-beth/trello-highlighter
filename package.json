{
  "name": "trello-highlighter",
  "version": "0.1.0",
  "scripts": {
    "dev": "npm run cp:manifest && parcel watch src/*.js --dist-dir dist --no-source-maps --no-content-hash",
    "build:js": "parcel build src/*.js --dist-dir dist --no-source-maps --no-content-hash",
    "cp:manifest": "cpx src/manifest.json dist/",
    "build": "npm run build:js && npm run cp:manifest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smile-beth/trello-highlighter.git"
  },
